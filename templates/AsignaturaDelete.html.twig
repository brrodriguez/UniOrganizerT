{% extends "layout.html.twig" %}

{% block title %}Asignatura Delete{% endblock %}
{% block body %}
	<div class="container" >
            <form method="POST" action="../Controllers/ASIGNATURA_Controller.php?accion=eliminar&amp;id={{ datos['idAsignatura'] }}">
                <div class="form-group" >
                    <label class="control-label" >{{ strings['Eliminar Asignatura'] }}</label><br>
                </div>
               
                
                <div class="form-group">
                    <label class="control-label" >{{ strings['nombreAsignatura'] }}</label><br>
                    <input class="form" id="nombreAsignatura" name="nombreAsignatura" size="50" type="text" readonly="true" value="{{ datos['nombreAsignatura'] }}">
                </div>             

                <br>
				
				<button type="submit" class="btn btn-primary">{{ strings['Borrar'] }}</button>
                <a class="form-link" href='{{ volver }}'>{{ strings['Volver'] }} </a>
            </form>
        </div>
{% endblock %}